<?xml version="1.0" encoding="ISO-8859-1"?>
<testdefinition version="0.1">
  <suite name="quillimagefilter-tests" domain="application-domain">
    <description>Automated test cases for quill image filters</description>

    <set name="quill-image-filter-auto-contrast-tests" feature="auto contrast">
      <description>quill image filter auto contrast</description>
      <case name="ut_autocontrast"  type="Functional" level="Component">
	<step>/usr/lib/quillimagefilter-tests/ut_autocontrast </step>
      </case>
      </set>
     <set name="quill-image-filter-auto-level-tests" feature="auto level">
       <description>quill image filter auto level</description>
    <case name="ut_autolevels"  type="Functional" level="Component">
	<step>/usr/lib/quillimagefilter-tests/ut_autolevels </step>
      </case>
     </set>

